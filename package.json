{
  "name": "quiz-master",
  "version": "1.0.0",
  "description": "A React-based quiz application with Formik for form handling and Yup for validation.",
  "keywords": [
    "react",
    "formik",
    "forms",
    "quiz"
  ],
  "main": "index.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "formik": "2.4.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "react-scripts": "4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
